<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="InterSemiBold">
        <item name="android:fontWeight">600</item>
        <item name="android:fontFamily">@font/font_inter_semibold</item>
    </style>

    <style name="InterSemiBold.Gray700.S20" parent="InterSemiBold">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/gray_700</item>
    </style>

    <style name="InterSemiBold.Primary.S40" parent="InterSemiBold">
        <item name="android:textSize">40sp</item>
        <item name="android:textColor">@color/primary</item>
    </style>

    <style name="InterMedium">
        <item name="android:fontWeight">500</item>
        <item name="android:fontFamily">@font/font_inter_medium</item>
    </style>

    <style name="InterMedium.White.S18" parent="InterMedium">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="InterMedium.Black.S16" parent="InterMedium">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="InterMedium.Gray400.S16" parent="InterMedium">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/gray_400</item>
    </style>

    <style name="InterMedium.Gray200.S16" parent="InterMedium">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/gray_200</item>
    </style>

    <style name="InterMedium.Gray400.S11" parent="InterMedium">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/gray_400</item>
    </style>
</resources>